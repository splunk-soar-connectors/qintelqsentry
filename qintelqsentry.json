{
    "appid": "a9561e88-1764-42bd-a4d5-8bb2e728e54c",
    "name": "Qintel QSentry",
    "description": "This app retrieves IP reputation and anonymization data from Qintel's QSentry platform",
    "type": "reputation",
    "product_vendor": "Qintel",
    "logo": "logo_qintelqsentry.svg",
    "logo_dark": "logo_qintelqsentry_dark.svg",
    "product_name": "Qintel QSentry",
    "product_version_regex": ".*",
    "publisher": "Qintel, LLC",
    "license": "Copyright (c) 2022 Qintel, LLC",
    "app_version": "1.0.0",
    "utctime_updated": "2021-11-16T14:25:46.922845Z",
    "package_name": "phantom_qintelqsentry",
    "main_module": "qintelqsentry_connector.py",
    "min_phantom_version": "5.0.0",
    "app_wizard_version": "1.0.0",
    "python_version": "3",
    "fips_compliant": true,
    "configuration": {
        "token": {
            "description": "API Token",
            "data_type": "password",
            "required": true,
            "order": 0
        },
        "remote": {
            "description": "QSentry API URL (Optional)",
            "data_type": "string",
            "default": "https://api.qsentry.qintel.com",
            "order": 1
        }
    },
    "actions": [
        {
            "action": "test connectivity",
            "identifier": "test_connectivity",
            "description": "Validate the asset configuration for connectivity using supplied configuration",
            "type": "test",
            "read_only": true,
            "parameters": {},
            "output": [],
            "versions": "EQ(*)"
        },
        {
            "action": "ip reputation",
            "identifier": "ip_reputation",
            "description": "Queries for IP intelligence data",
            "type": "investigate",
            "read_only": true,
            "parameters": {
                "ip": {
                    "description": "IP to query",
                    "data_type": "string",
                    "required": true,
                    "primary": true,
                    "contains": [
                        "ip"
                    ],
                    "order": 0
                }
            },
            "render": {
                "type": "table",
                "title": "Threat Info"
            },
            "output": [
                {
                    "data_path": "action_result.parameter.ip",
                    "data_type": "string",
                    "contains": [
                        "ip"
                    ],
                    "example_values": [
                        "1.2.3.4"
                    ]
                },
                {
                    "column_name": "ASN",
                    "column_order": 0,
                    "data_path": "action_result.data.*.asn",
                    "data_type": "string",
                    "example_values": [
                        "12345"
                    ]
                },
                {
                    "column_name": "ASN Name",
                    "column_order": 1,
                    "data_path": "action_result.data.*.asn_name",
                    "data_type": "string",
                    "example_values": [
                        "Some Service Provider"
                    ]
                },
                {
                    "column_name": "Descriptions",
                    "column_order": 4,
                    "data_path": "action_result.data.*.descriptions",
                    "data_type": "string"
                },
                {
                    "column_name": "Last Seen",
                    "column_order": 2,
                    "data_path": "action_result.data.*.last_seen",
                    "data_type": "string",
                    "example_values": [
                        "2021-11-16T15:05:32.070497"
                    ]
                },
                {
                    "column_name": "Tags",
                    "column_order": 3,
                    "data_path": "action_result.data.*.tags",
                    "data_type": "string",
                    "example_values": [
                        "vpn,botnet"
                    ]
                },
                {
                    "data_path": "action_result.status",
                    "data_type": "string",
                    "example_values": [
                        "success",
                        "failed"
                    ]
                },
                {
                    "data_path": "action_result.message",
                    "data_type": "string"
                },
                {
                    "data_path": "action_result.summary",
                    "data_type": "string"
                },
                {
                    "data_path": "summary.total_objects",
                    "data_type": "numeric",
                    "example_values": [
                        1
                    ]
                },
                {
                    "data_path": "summary.total_objects_successful",
                    "data_type": "numeric",
                    "example_values": [
                        1
                    ]
                }
            ],
            "versions": "EQ(*)"
        }
    ]
}
